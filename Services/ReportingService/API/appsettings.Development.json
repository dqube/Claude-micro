{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information",
      "BuildingBlocks.API": "Debug",
      "ReportingService": "Debug"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=ReportingServiceDb;Trusted_Connection=true;MultipleActiveResultSets=true;",
    "Redis": "localhost:6379"
  },
  "Authentication": {
    "Jwt": {
      "SecretKey": "dev-secret-key-for-reporting-service-must-be-at-least-32-characters-long-and-secure",
      "Issuer": "ReportingService.Development",
      "Audience": "ReportingService-API.Development",
      "ExpiryMinutes": 240
    }
  },
  "Cors": {
    "AllowedOrigins": ["http://localhost:3000", "https://localhost:3000", "http://localhost:5001", "https://localhost:5001", "http://localhost:7001", "https://localhost:7001"]
  },
  "SecurityHeaders": {
    "EnableHsts": false
  },
  "Cache": {
    "DefaultExpiration": "00:05:00"
  }
} 