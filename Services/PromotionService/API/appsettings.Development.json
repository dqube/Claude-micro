{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information",
      "PromotionService": "Debug",
      "BuildingBlocks": "Debug"
    }
  },
  "OpenTelemetry": {
    "Exporters": {
      "Console": {
        "Enabled": true
      },
      "Otlp": {
        "Enabled": false
      }
    }
  },
  "Cors": {
    "AllowedOrigins": ["*"]
  },
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Documentation": {
    "UseScalar": true,
    "UseSwagger": true,
    "IncludeXmlComments": true
  },
  "Validation": {
    "ValidateOnStartup": false,
    "StrictMode": false
  },
  "RateLimiting": {
    "PermitLimit": 1000,
    "Window": "00:01:00"
  }
} 